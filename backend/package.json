{
  "name": "dark-matter-assignment",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "nodemon --exec ts-node src/index.ts",
    "prod": "npm run build && npm run start"
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "^10.12.7",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.2.0",
    "inversify-logger-middleware": "^3.1.0",
    "nodemon": "^1.18.6",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.0.0",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
